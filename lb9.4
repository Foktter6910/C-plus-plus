#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the array size: ";
    cin >> n;

    int *a = new int[n];
    int i;

    cout << "Enter " << n << " numbers:\n";
    for (i = 0; i < n; i++) {
        cin >> a[i];
    }

    int A;
    cout << "Enter A: ";
    cin >> A;

    int k = 0;
    for (i = 0; i < n; i++) {
        if (a[i] > A) {
            k++;
        }
    }

    cout << "Number of elements greater than A: " << k << endl;

    delete[] a;
    return 0;
}

