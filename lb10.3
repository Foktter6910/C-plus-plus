#include <iostream>
using namespace std;

int main() {
    const int m = 5, n = 7;
    int a[m][n];
    int i, j;

    cout << "Enter a 5x7 array:\n";
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }

    for (i = 0; i < m; i++) {
        int sum = 0;
        for (j = 0; j < n; j++) {
            sum += a[i][j];
        }
        cout << "Row " << i << " sum: " << sum << endl;
    }

    return 0;
}
