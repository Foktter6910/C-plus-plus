#include <iostream>
using namespace std;

int main() {
    const int m = 4, n = 6;
    int a[m][n];
    int i, j;

    cout << "Enter a 4x6 array:\n";
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }

    for (j = 0; j < n; j++) {
        int sum = 0;
        for (i = 0; i < m; i++) {
            sum += a[i][j];
        }
        cout << "Column " << j << " sum: " << sum << endl;
    }

    return 0;
}
