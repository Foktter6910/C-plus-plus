#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double a = 3;
    double b = 0.151;

    double numerator = 1 - pow(cos((b / a) * 3.141592653589793), 2);
    double denominator = 0.5 * exp(b) + 0.312 * exp(a);
    double result = numerator / denominator;

    cout << "Result: " << result << endl;

    return 0;
}

