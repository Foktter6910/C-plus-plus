#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double sum = 0;
    for (int i = 10; i <= 20; i++) {
        sum += sqrt(i); // Додаємо квадратний корінь з поточного числа
    }
    cout << "The sum of the roots of the numbers from 10 to 20: " << sum << endl;
    return 0;
}
