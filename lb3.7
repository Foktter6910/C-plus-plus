#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    float a, b, c, d, e;
    cin >> a >> b >> c >> d >> e;

    cout << a << endl;                       
    cout << fixed << setprecision(2) << b << endl;  
    cout << setprecision(6) << c << endl;    
    cout << setprecision(2) << d << endl;    
    cout << static_cast<int>(e) << endl;      

    return 0;
}
